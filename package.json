{
  "name": "erixhens.me",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "homepage": "https://github.com/ErixM/erixhens.me.git",
  "license": "MIT",
  "scripts": {
    "deploy2": "gatsby build --prefix-paths && gh-pages -d public",
    "push": "git subtree push --prefix public origin gh-pages",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "analyze": "cross-env ANALYZE_BUNDLE_SIZE=true gatsby build"
  },
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "@mdx-js/mdx": "^1.5.1",
    "@mdx-js/react": "^1.5.1",
    "gatsby": "^2.19.18",
    "gatsby-background-image": "^0.9.11",
    "gatsby-image": "^2.2.34",
    "gatsby-plugin-amplitude-analytics": "^2.0.0",
    "gatsby-plugin-emotion": "^4.1.16",
    "gatsby-plugin-manifest": "^2.2.41",
    "gatsby-plugin-mdx": "^1.0.59",
    "gatsby-plugin-offline": "^3.0.35",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-sharp": "^2.3.5",
    "gatsby-plugin-sitemap": "^2.2.27",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-remark-images": "^3.1.35",
    "gatsby-source-filesystem": "^2.1.40",
    "gatsby-source-instagram": "^0.6.1",
    "gatsby-transformer-sharp": "^2.3.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.0",
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
