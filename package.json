{
  "name": "erixhens.me",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "homepage": "https://github.com/ErixM/erixhens.me.git",
  "license": "MIT",
  "scripts": {
    "push":"git subtree push --prefix public origin gh-pages",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "deploy": "gatsby build && git subtree push --prefix dist origin gh-pages",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@mdx-js/mdx": "^1.5.5",
    "@mdx-js/react": "^1.5.5",
    "flubber": "^0.4.2",
    "gatsby": "^2.18.12",
    "gatsby-plugin-emotion": "^4.1.21",
    "gatsby-plugin-mdx": "^1.0.70",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-source-filesystem": "^2.1.48",
    "popmotion": "^8.7.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "gh-pages": "^2.2.0",
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
